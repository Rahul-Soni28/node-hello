pipeline{
    agent any
    stages{
        stage("Build"){
            steps{
                sh 'npm pack'
                sh 'set BUILD_ID=dontKillMe nohup npm run start&'
            }
        }
        stage("Test"){
            echo "====++++Testing++++===="
        }
        stage("build"){
            echo "====++++building++++===="
        }
    }
}