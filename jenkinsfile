pipeline{
    agent any
    stages{
        stage("Build"){
            steps{
                sh 'npm pack'
                sh 'set BUILD_ID=dontKillMe nohup npm run start &'
            }
        }
        stage("Test"){
            steps{
            echo "====++++Testing++++===="
            }
        }
        stage("build"){
            steps{
            echo "====++++building++++===="
            }
        }
    }
}