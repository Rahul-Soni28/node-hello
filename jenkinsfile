pipeline{
    agent any
    stages{
        stage("Build"){
            steps{
                sh 'npm pack'
                sh 'nohup npm run start'
            }
        }
        stage("Test"){
            steps{
            echo "====++++Testing++++===="
            }
        }
        stage("build"){
            steps{
            echo "====++++building++++===="
            }
        }
    }
}